package makeup.vo;

public class PageModel {
	public static final int MAX_RECORD = 20;
    private int totalPages;
    private int page;
    private int maxRecord;
    private long totalRecords;
    private int RECORD_LIMIT;
    private int center;
    private int edge;
    
    public PageModel()
    {
        totalPages = 0;
        page = 1;
        maxRecord = 20;
        totalRecords = 0L;
        RECORD_LIMIT = 100;
        center = 9;
        edge = 2;
    }

    public PageModel(int i)
    {
        totalPages = 0;
        page = 1;
        maxRecord = 20;
        totalRecords = 0L;
        RECORD_LIMIT = 100;
        center = 9;
        edge = 2;
        maxRecord = i;
    }

    public PageModel(int i, int j)
    {
        totalPages = 0;
        page = 1;
        maxRecord = 20;
        totalRecords = 0L;
        RECORD_LIMIT = 100;
        center = 9;
        edge = 2;
        page = i;
        maxRecord = j;
    }

    public PageModel(int i, int j, int k)
    {
        totalPages = 0;
        page = 1;
        maxRecord = 20;
        totalRecords = 0L;
        RECORD_LIMIT = 100;
        center = 9;
        edge = 2;
        page = i;
        maxRecord = j;
        center = k;
    }
    
    
	public int getTotalPages() {
		return totalPages;
	}
	public void setTotalPages(int totalPages) {
		this.totalPages = totalPages;
	}
	public int getPage() {
		return page;
	}
	public void setPage(int page) {
		this.page = page;
	}
	public int getMaxRecord() {
		return maxRecord;
	}
	public void setMaxRecord(int maxRecord) {
		this.maxRecord = maxRecord;
	}
	public long getTotalRecords() {
		return totalRecords;
	}
	public void setTotalRecords(long totalRecords) {
		this.totalRecords = totalRecords;
	}
	public int getCenter() {
		return center;
	}
	public void setCenter(int center) {
		this.center = center;
	}
	public int getEdge() {
		return edge;
	}
	public void setEdge(int edge) {
		this.edge = edge;
	}
	public int getRECORD_LIMIT() {
		return RECORD_LIMIT;
	}
	public void setRECORD_LIMIT(int rECORD_LIMIT) {
		RECORD_LIMIT = rECORD_LIMIT;
	}
    
    

}
