package makeup.action.admin;

import java.util.List;
import com.opensymphony.xwork2.ActionSupport;

import makeup.model.Intro;
import makeup.service.IntroService;

public class IntroAction extends ActionSupport{

	
	private static final long serialVersionUID = 9021657869226847989L;

	private IntroService introService = new IntroService();
	private Intro intro;
	private List<Intro> list;
	private String content;//公司简介
	private String savepath;//照片
	
	public IntroService getIntroService() {
		return introService;
	}
	public void setIntroService(IntroService introService) {
		this.introService = introService;
	}
	public Intro getIntro() {
		return intro;
	}
	public void setIntro(Intro intro) {
		this.intro = intro;
	}
	public List<Intro> getList() {
		return list;
	}
	public void setList(List<Intro> list) {
		this.list = list;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public static long getSerialversionuid() {
		return serialVersionUID;
	}
	public String getSavepath() {
		return savepath;
	}
	public void setSavepath(String savepath) {
		this.savepath = savepath;
	}
	
	
}
